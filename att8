#include<stdio.h>
#include<math.h>

main(){
	int idade, tempoTrab;
	
	printf("Qual a tua idade?:\n");
	scanf("%d", &idade);
	
	printf("A quanto tempo voce trabalha?:\n");
	scanf("%d", &tempoTrab);

	if((idade >= 65) || (tempoTrab >= 30)){
		printf("pode aposentar velho");
	}
	else if((idade >= 60) && (tempoTrab >= 25)){
		printf("pode aposentar velho");
	}
	else{
		printf("falta pouco pra jogar no INSS");
	}
}
